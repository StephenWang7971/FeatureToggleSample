<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="propertyConfigurer" class="com.sec.framework.util.FeatureTogglePropertyPlaceHolderConfigurer">
        <property name="locations">
            <list>
                <value>/WEB-INF/config/feature-toggle.properties</value>
                <!--要是有多个配置文件，只需在这里继续添加即可 -->
            </list>
        </property>
    </bean>
</beans>